   "source": [
    "from scipy.special import lambertw\n",
    "import numpy as np\n",
    "X = [0*np.exp(0), 1*np.exp(1), 2*np.exp(2)]\n",
    "Y = [lambertw(X[0]).real, lambertw(X[1]).real, lambertw(X[2]).real]\n",
    "\n",
    "print(\"X:\", [f\"{x:.4f}\" for x in X])\n",
    "print(\"Y:\", [f\"{y:.4f}\" for y in Y])"
   ]
